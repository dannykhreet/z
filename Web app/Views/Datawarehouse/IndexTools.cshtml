@model WebApp.ViewModels.DatawarehouseToolsViewModel
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
    Layout = "_Layout_template";
}


<div class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <a href="/datawarehouse">Back to datawarehouse</a>
                <h1 class="m-0 text-dark">Datawarehouse tools</h1>
            </div><!-- /.col -->
            <div class="col-sm-6 d-flex flex-row-reverse">
            </div><!-- /.col -->
        </div><!-- /.row -->
    </div><!-- /.container-fluid -->
</div>

<section class="content">
    <div class="container-fluid">

        <div class="card">
            <div class="card-header">
                <h3 id="cardHeader" class="card-title d-none d-sm-inline-block">Overview<span id="counter">&nbsp;</span></h3>
                <div class="card-tools">
                    <div class="input-group input-group-sm">
                    </div>
                </div>
            </div>

        </div>

        <div class="row pt-1" style="height:calc(100vh - 270px);overflow:auto;">

            <div class="col-lg-12 col-xl-12">
                <div id="contentBody" style="height: calc(100vh - 220px);">

                    <div class="row no-gutters">

                        <div class="card w-100 results">
                            <div class="card-body p-0">
                                <div class="col-12 p-1">
                                    <div class="p-1">
                                        <div class="row" style="height:100px;">
                                            <div class="col-sm-1 col-md-1 col-lg-1 col-xl-1">
                                                <div><small></small></div>
                                                <div style="height:60px;vertical-align:middle; text-align:center;" title="">
                                                    <i class="fa-solid fa-cloud-arrow-up" style="margin-top:25px; font-size: 40px;color:#808080" title="Data Import"></i>
                                                </div>
                                            </div>
                                            <div class="col-sm-10 col-md-10 col-lg-10 col-xl-10">
                                                <div class="row">
                                                    <div class="col-lg-12">
                                                        <h5 class="mt-3 mt-sm-0">Data import</h5>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-lg-12 d-flex justify-content-between flex-column">
                                                        <div class="row">
                                                            <div class="col-md-12 col-lg-12 text-muted">
                                                                <small>
                                                                    Data import will allow you to import data from the EZGO platform into the DataWarehouse. This is done by using the API calls to the EZGO platform and then storing the data in the Datawarehouse.
                                                                    This will more or less simulate and execute the logic as the normal data snapper of the datawarehouse would do. Import is platform based. When running cross platform, this will need to be run locally.
                                                                </small>
                                                            </div>
                                                            <div class="col-md-12 col-lg-12 text-muted">
                                                                <small>

                                                                </small>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-sm-1 col-md-1 col-lg-1 col-xl-1" style="text-align: right;">
                                                <a href="/datawarehouse/tools/import" id="btn_import" class="btn btn-ezgo btn-sm mr-2 shadow-sm mt-auto"><i class="fas fa-edit"></i></a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="card w-100 results">
                            <div class="card-body p-0">
                                <div class="col-12 p-1">
                                    <div class="p-1">
                                        <div class="row" style="height:100px;">
                                            <div class="col-sm-1 col-md-1 col-lg-1 col-xl-1">
                                                <div><small></small></div>
                                                <div style="height:60px;vertical-align:middle; text-align:center;" title="">
                                                    <i class="fa-solid fa-chart-simple" style="margin-top:25px; font-size: 40px;color:#808080" title="Statistics"></i>
                                                </div>
                                            </div>
                                            <div class="col-sm-10 col-md-10 col-lg-10 col-xl-10">
                                                <div class="row">
                                                    <div class="col-lg-12">
                                                        <h5 class="mt-3 mt-sm-0">Statistics</h5>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-lg-12 d-flex justify-content-between flex-column">
                                                        <div class="row">
                                                            <div class="col-md-12 col-lg-12 text-muted">
                                                                <small>
                                                                    Retrieve statistics from the datawarehouse. This will allow you to see how many items are available per table. Tool can be used to view the output as would normally be done when running manual API calls to view if the import adds data. 
                                                                    The retrieval will be done every number of seconds and what data that can be retrieved can be changed. 
                                                                </small>
                                                            </div>
                                                            <div class="col-md-12 col-lg-12 text-muted">
                                                                <small>

                                                                </small>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-sm-1 col-md-1 col-lg-1 col-xl-1" style="text-align: right;">
                                                <a href="/datawarehouse/tools/statistics" id="btn_noc" class="btn btn-ezgo btn-sm mr-2 shadow-sm mt-auto"><i class="fas fa-edit"></i></a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>

</section>


@section Scripts {

    <script>


    </script>

}
