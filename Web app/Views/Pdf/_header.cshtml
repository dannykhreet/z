@model string

@{
    Layout = null;
}
<!doctype html>
<html lang="en">
<head>
    <style>

        #header {
            font-family: -apple-system,BlinkMacSystemFont,"Segoe UI",Roboto,'Noto Sans Malayalam','Noto Sans KR','Noto Sans Thai'"Helvetica Neue",Arial,sans-serif,"Apple Color Emoji","Segoe UI Emoji","Segoe UI Symbol";
            font-size: 18px;
            white-space: nowrap;
            width: 100%;
            height: 2.75rem;
            color: white;
            background-color: #c4c4c4;
            border-spacing: 0 0;
            border-collapse: collapse;
            vertical-align: bottom;
        }
    </style>
</head>
<body>
    <div id="header" style="width: 100%; top: 0px; left: 0px; right: 0px">
        <div style="width: 100%; top: 0; height: auto">
            <div style="text-align: left; text-transform: uppercase; padding-left: 2rem; padding-bottom: 0.8rem; padding-top: 0.5rem">
                <strong>@Model</strong>
            </div>
        </div>
    </div>
</body>
</html>
