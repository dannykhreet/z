<?xml version="1.0" encoding="utf-8" ?>
<Frame xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:controls="clr-namespace:EZGO.Maui.Controls;assembly=EZGO.Maui"
             x:Class="EZGO.Maui.Controls.Images.RoundUserImage"
             HorizontalOptions="Center" VerticalOptions="Center"
             HeightRequest="40" WidthRequest="40"        
             CornerRadius="20"
             HasShadow="False"
             Padding="0"
             IsClippedToBounds="True">
    <controls:CustomImageOnline
        HidePlaceholder="False"
        x:Name="UserImage" Aspect="AspectFill" HorizontalOptions="StartAndExpand" Placeholder="profile.png" WidthRequest="40" HeightRequest="40" />
</Frame>
